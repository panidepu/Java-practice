import java.util.Scanner;

public class PrimeNumber
{
    public static void main(String args[])
    {
        int num, i, count=0;
 Scanner scan = new Scanner(System.in);
		
        System.out.print("Enter a Number : ");
        num = scan.nextInt();
        for(i=2; i<num; i++)
        {
            if(num%i == 0)
            {
                count++;
                break;
            }
        }
    }}
    